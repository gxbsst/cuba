<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<queries xmlns="http://schemas.haulmont.com/cuba/rest-queries.xsd">
    <query entity="sct$Mqtt"
           name="mqtt-query"
           view="mqtt-view">
        <jpql>
            <![CDATA[select e from cuba$Mqtt e where e.topic = :topic]]>
        </jpql>
        <params>
            <param name="topic"
                   type="java.lang.String"/>
        </params>
    </query>
</queries>
