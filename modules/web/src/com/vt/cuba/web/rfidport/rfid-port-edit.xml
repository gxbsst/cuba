<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.vt.cuba.web.rfidport.RfidPortEdit"
        datasource="rfidPortDs"
        focusComponent="fieldGroup"
        messagesPack="com.vt.cuba.web.rfidport">
    <dsContext>
        <datasource id="rfidPortDs"
                    class="com.vt.cuba.entity.RfidPort"
                    view="_local"/>
    </dsContext>
    <dialogMode forceDialog="true"/>
    <layout spacing="true">
        <groupBox spacing="true">
            <fieldGroup id="fieldGroup"
                        datasource="rfidPortDs">
                <column width="250px">
                    <field property="name"/>
                    <field property="antenna"/>
                    <field property="valid_rssi"/>
                </column>
            </fieldGroup>
            <frame id="windowActions"
                   height="100%"
                   screen="editWindowActions"/>
        </groupBox>
    </layout>
</window>
